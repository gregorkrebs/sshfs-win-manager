{
  "name": "sshfs-win-manager",
  "version": "1.3.1",
  "author": "Evandro Araujo <evandroprogram@gmail.com>",
  "description": "A GUI for SSHFS-Win (https://github.com/billziss-gh/sshfs-win)",
  "license": "MIT",
  "main": "electron/main.js",
  "engines": {
    "node": ">=22"
  },
  "scripts": {
    "dev": "concurrently -k \"vite\" \"wait-on http://localhost:5173 && electron .\"",
    "build:renderer": "vite build",
    "build": "npm run build:renderer && electron-builder",
    "build:dir": "npm run build:renderer && electron-builder --dir"
  },
  "build": {
    "productName": "SSHFS-Win Manager",
    "appId": "dev.babum.apps.sshfs-win-manager",
    "artifactName": "sshfs-win-manager-setup-v${version}.${ext}",
    "nsis": {
      "oneClick": true,
      "installerIcon": "build/icons/setup-icon.ico",
      "uninstallDisplayName": "SSHFS-Win Manager"
    },
    "directories": {
      "output": "build"
    },
    "files": [
      "dist/**/*",
      "electron/**/*",
      "static/**/*"
    ],
    "win": {
      "icon": "build/icons/app-icon.ico"
    }
  },
  "dependencies": {
    "uuid": "^11.1.0",
    "v-tooltip": "^2.1.3",
    "vue": "^2.7.16",
    "vue-js-toggle-button": "^1.3.3",
    "vue-router": "^3.6.5",
    "vue-switches": "^2.0.1",
    "vuedraggable": "^2.24.3",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0"
  },
  "devDependencies": {
    "concurrently": "^9.2.0",
    "electron": "^30.5.1",
    "electron-builder": "^24.13.3",
    "less": "^4.2.2",
    "sass": "^1.77.8",
    "vite": "^5.4.14",
    "@vitejs/plugin-vue2": "^2.3.3",
    "wait-on": "^8.0.2"
  }
}
